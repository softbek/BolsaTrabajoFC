<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="model.Habilidad" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="HABILIDAD">
    <id column="HAB_ID" name="id">
      <generator class="native"/>
    </id>
    <property column="NOMBRE" name="nombre"/>
    <property column="CERTIFICADO" name="certificado"/>
    <property column="DATE" name="creacion" type="timestamp"/>
    <!--<many-to-one name="curriculum" class="model.Curriculum" column="CV_ID" not-null="true" foreign-key="FK_CV_ID"/>-->
  </class>
</hibernate-mapping>
