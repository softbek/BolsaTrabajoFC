<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="model.MessageStore" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="MESSAGE_STORE">
    <id column="ID" name="id">
      <generator class="native"/>
    </id>
    <property column="MENSAJE" name="message"/>
    <property column="DATE" name="date" type="timestamp"/>
  </class>
</hibernate-mapping>
